{
    "name": "chi-teck/commander",
    "description": "Simple and fast CLI tool for Drupal",
    "license": "GPL-2.0-or-later",
    "bin": [
        "bin/commander"
    ],
    "autoload": {
        "psr-4": {
            "Commander\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1.0",
        "ext-json": "*",
        "drupal/core": "^8.7.0",
        "symfony/console": "^3.4.0",
        "symfony/dependency-injection": "^3.4.0",
        "symfony/http-foundation": "~3.4.0",
        "webflo/drupal-finder": "^1.1.0"
    },
    "extra": {
        "commands": [
            "Commander\\Command\\ClearCacheCommand",
            "Commander\\Command\\SiteStatusCommand",
            "Commander\\Command\\RunCronCommand"
        ]
    }
}
